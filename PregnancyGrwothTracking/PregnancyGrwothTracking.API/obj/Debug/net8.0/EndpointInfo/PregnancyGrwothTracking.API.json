        "additionalProperties": false
      },
      "RegisterRequestDto": {
        "required": [
          "dob",
          "email",
          "fullName",
          "password",
          "phone",
          "username"
        ],
        "type": "object",
        "properties": {
          "username": {
            "maxLength": 20,
            "minLength": 6,
            "pattern": "^[a-zA-Z0-9_]+$",
            "type": "string"
          },
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "password": {
            "minLength": 6,
            "pattern": "^(?=.*[A-Za-z])(?=.*\\d)[A-Za-z\\d]{6,}$",
            "type": "string"
          },
          "fullName": {
            "maxLength": 50,
            "minLength": 1,
            "type": "string"
          },
          "dob": {
            "type": "string",
            "format": "date"
          },
          "phone": {
            "minLength": 1,
            "pattern": "^0\\d{9}$",
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "UserCreateDto": {
        "required": [
          "available",
          "dob",
          "email",
          "fullName",
          "password",
          "phone",
          "roleId",
          "userName"
        ],
        "type": "object",
        "properties": {
          "userName": {
            "maxLength": 30,
            "minLength": 5,
            "pattern": "^(?![0-9]+$)(?!.*\\s)(?!.*[_\\.]$)(?!^[_\\.])(?!.*[_\\.]{2,})[a-zA-Z0-9._]+$",
            "type": "string"
          },
          "fullName": {
            "maxLength": 30,
            "minLength": 2,
            "pattern": "^[a-zA-Z\\s]+$",
            "type": "string"
          },
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "password": {
            "maxLength": 30,
            "minLength": 8,
            "pattern": "^(?=.*[a-z])(?=.*[A-Z])(?=.*\\d)[A-Za-z\\d]+$",
            "type": "string"
          },
          "dob": {
            "type": "string",
            "format": "date"
          },
          "phone": {
            "minLength": 1,
            "pattern": "^0[0-9]{9}$",
            "type": "string"
          },
          "available": {
            "type": "boolean"
          },
          "roleId": {
            "maximum": 3,
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "UserResponseDto": {
        "type": "object",
